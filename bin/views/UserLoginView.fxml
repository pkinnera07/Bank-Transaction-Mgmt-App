<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserLoginController" >
    <children>
        <Label layoutX="130.0" layoutY="24.0" text="User Login" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="usernameField" layoutX="106.0" layoutY="71.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="106.0" layoutY="108.0" promptText="Password" />
        <Button fx:id="loginButton" layoutX="132.0" layoutY="154.0" mnemonicParsing="false" onAction="#handleUserLogin" text="Login" />
    </children>
</AnchorPane>

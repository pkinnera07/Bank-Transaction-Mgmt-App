<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController" >
    <children>
        <Text layoutX="230.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose Login Type">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <Button fx:id="adminLoginButton" layoutX="217.0" layoutY="134.0" mnemonicParsing="false" onAction="#openAdminLogin" text="Administrator Login" />
        <Button fx:id="userLoginButton" layoutX="217.0" layoutY="195.0" mnemonicParsing="false" onAction="#openUserLogin" text="User Login" />
        <Button fx:id="bankLoginButton" layoutX="217.0" layoutY="250.0" mnemonicParsing="false" onAction="#openBankLogin" text="Bank Login" />
    </children>
    
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</AnchorPane>
